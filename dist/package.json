{
  "name": "@nhahv/wordpress-seo-yoastseo",
  "description": "Yoast clientside page analysis",
  "homepage": "https://github.com/Yoast/",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "keywords": [
    "Yoast",
    "SEO",
    "text analysis"
  ],
  "main": "index.js",
  "types": "index.d.ts",
  "license": "GPL-3.0",
  "version": "1.91.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/nhahv/wordpress-seo-yoastseo.git"
  },
  "scripts": {
    "start": "npm start --prefix examples/webpack",
    "build": "grunt publish",
    "lint": "grunt check",
    "translate": "grunt build:translations",
    "po2json": "grunt po2json",
    "test": "jest --runInBand",
    "install-ci": "grunt get-premium-configuration",
    "prepublishOnly": "grunt publish"
  },
  "browserslist": [
    "defaults"
  ],
  "engines": {
    "node": ">=8.0.0"
  },
  "bugs": {
    "url": "https://github.com/Yoast/js-text-analysis/issues"
  },
  "dependencies": {
    "@wordpress/autop": "^2.0.2",
    "@yoast/feature-flag": "^0.5.2",
    "htmlparser2": "^3.9.2",
    "jed": "^1.1.0",
    "lodash-es": "^4.17.10",
    "loglevel": "^1.6.1",
    "parse5": "^5.1.0",
    "sassdash": "0.9.0",
    "tokenizer2": "^2.0.1"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "parserOptions": {
      "sourceType": "module",
      "allowImportExportEverywhere": "true"
    }
  },
  "browserify": {
    "transform": [
      [
        "babelify"
      ]
    ]
  },
  "yoast": {
    "premiumConfiguration": ""
  }
}